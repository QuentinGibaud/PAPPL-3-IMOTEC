<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    package="com.example.gibaud.applicationprojet">

    <application
        android:allowBackup="true"
        android:icon="@drawable/imotec"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityRappel"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityMesure"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".ActivityAlgo"></activity>
        <activity android:name=".ActivityVersion"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>